#include <iostream>
#include <vector>
using namespace std;
class Solution {
public:
    string longestCommonPrefix(vector<string>& strs) {
       if (strs.empty()) {
        return "";
       }
       string prefix = strs[0];
       for (int i = 1; i < strs.size(); i++) {
        string currentword = strs[i];

        while(currentword.find(prefix) != 0) {
            prefix = prefix.substr(0, prefix.size() - 1);
            if (prefix.empty()) {
                return "";
            }
        }

       }
       return prefix;
    }

};
